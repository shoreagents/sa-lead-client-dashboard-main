  industry             String?
  department           String?
  work_type            String                 @default("full-time")
  currency             String                 @default("PHP")
  salary_type          String                 @default("monthly")
  status               String                 @default("new_request")
  views                Int                    @default(0)
  applicants           Int                    @default(0)
